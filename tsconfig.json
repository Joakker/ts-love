{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext"],
    "types": ["lua-types/jit", "love-typescript-definitions"],
    "moduleResolution": "node",
    "strict": true,
    "rootDir": "src/",
    "outDir": "game/",
    "plugins": [{ "name": "typescript-tstl-plugin" }]
  },
  "tstl": {
    "luaTarget": "JIT"
  }
}
